// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Specialist {
  id   String @id @default(uuid())
  gender String
  firstName String
  lastName String
}

model Therapy {
  id   String @id @default(uuid())
  price Int
}
